{
    "version": 1,
    "formatters": {
      "simple": {
        "format": "%(asctime)s %(levelname)s:%(name)s:%(message)s"
      }
    },
    "handlers": {
      "stderr": {
        "class": "logging.StreamHandler",
        "level": "INFO",
        "formatter": "simple",
        "stream": "ext://sys.stderr"
      }
    },
    "root": {
      "level": "INFO",
      "handlers": ["stderr"]
    }
  }
